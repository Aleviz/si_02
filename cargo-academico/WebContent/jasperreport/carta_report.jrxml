	<queryString language="SQL">
		<![CDATA[SELECT unidad.id_unidad AS unidad_id_unidad,
	unidad.nombre_unidad AS unidad_nombre_unidad,
	unidad.temario AS unidad_temario,
	unidad.objetivo AS unidad_objetivo,
	unidad.duracion AS unidad_duracion,
	unidad.ponderacion AS unidad_ponderacion,
	tipo_actividad.id_tipo_actividad AS tipo_actividad_id_tipo_actividad,
	tipo_actividad.descripcion AS tipo_actividad_descripcion,
	tipo_actividad.actividad AS tipo_actividad_actividad,
	materias.id_materia AS materias_id_materia,
	materias.materia AS materias_materia,
	materias.codigo AS materias_codigo,
	materias.departamento AS materias_departamento,
	materias.unidad_valorativa AS materias_unidad_valorativa,
	materias.prerequisito AS materias_prerequisito,
	materias.temario AS materias_temario,
	materias.corequisito AS materias_corequisito,
	actividades.id_actividad AS actividades_id_actividad,
	actividades.actividad AS actividades_actividad,
	actividades.descripcion AS actividades_descripcion,
	actividades.unidad AS actividades_unidad,
	actividades.ponderacion AS actividades_ponderacion,
	actividades.recursos AS actividades_recursos,
	actividades.tipo_actividad AS actividades_tipo_actividad,
	temario.id_temario AS temario_id_temario,
	temario.nombre_temario AS temario_nombre_temario,
	tema.id_tema AS tema_id_tema,
	tema.tema AS tema_tema,
	tema.unidad AS tema_unidad,
	tema.tiempo_tema AS tema_tiempo_tema
FROM tipo_actividad tipo_actividad
	INNER JOIN actividades actividades ON 
	 tipo_actividad.id_tipo_actividad = actividades.tipo_actividad 
	INNER JOIN unidad unidad ON 
	 actividades.unidad = unidad.id_unidad 
	INNER JOIN temario temario ON 
	 unidad.temario = temario.id_temario 
	INNER JOIN tema tema ON 
	 unidad.id_unidad = tema.unidad 
	INNER JOIN materias materias ON 
	 temario.id_temario = materias.temario
	 ]]>
	</queryString>
