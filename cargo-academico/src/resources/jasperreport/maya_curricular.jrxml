	<queryString language="SQL">
		<![CDATA[SELECT escuela.id_escuela AS esidescuela,
	escuela.nombre_escuela AS esnombreescuela,
	escuela.descripcion AS esdescripcion,
	escuela.campo_de_accion AS escampodeaccion,
	escuela.mision AS esmision,
	escuela.vision AS escvision,
	escuela.objetivo AS esobjetivo,
	escuela.telefono AS estelefono,
	facultad.facultad AS ffacultad,
	facultad.ubicacion AS fubicacion
FROM facultad facultad
	INNER JOIN escuela escuela ON 
	 facultad.id_facultad = escuela.id_facultad 
WHERE 
	 escuela.id_escuela = 1]]>
