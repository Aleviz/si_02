	<queryString language="SQL">
		<![CDATA[SELECT cargo_academico.escuela.id_escuela,
	cargo_academico.escuela.nombre_escuela,
	cargo_academico.escuela.telefono,
	cargo_academico.escuela.descripcion,
	cargo_academico.escuela.campo_de_accion,
	cargo_academico.escuela.mision,
