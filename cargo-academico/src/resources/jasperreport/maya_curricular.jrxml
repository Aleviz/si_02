	cargo_academico.escuela.objetivo,
	cargo_academico.facultad.facultad
FROM cargo_academico.escuela
	INNER JOIN cargo_academico.facultad ON 
	 cargo_academico.escuela.id_facultad = cargo_academico.facultad.id_facultad 
WHERE 
	 cargo_academico.escuela.id_escuela = 1]]>
